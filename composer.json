{
    "name": "calcasa/api",
    "version": "0.0.4",
    "description": "API client for the Calcasa Public API.",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://github.com/calcasa/api-php",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Calcasa",
            "homepage": "https://calcasa.nl"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "autoload": {
        "psr-4": { "Calcasa\\Api\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Calcasa\\Api\\Test\\" : "test/" }
    }
}